<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="assembly" value="C:\Users\Barry\Documents\Current Projects\tbs-developersuite\TheBoxSoftware.Reflection\bin\Debug\TheBoxSoftware.Reflection.dll" />
  </appSettings>

  <system.diagnostics>
    <trace autoflush="true"/>
