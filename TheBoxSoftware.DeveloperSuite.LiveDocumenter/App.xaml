<Application x:Class="TheBoxSoftware.DeveloperSuite.LiveDocumenter.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:pages="clr-namespace:TheBoxSoftware.DeveloperSuite.LiveDocumenter.Pages"
    xmlns:local="clr-namespace:TheBoxSoftware.DeveloperSuite.LiveDocumenter.Pages.Elements"
    xmlns:doc="clr-namespace:System.Windows.Documents;assembly=PresentationFramework"
    ShutdownMode="OnLastWindowClose" StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/DefaultStyles.xaml" />
                <ResourceDictionary Source="Resources/SummaryTable.xaml" x:Name="SummaryTableResources" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>